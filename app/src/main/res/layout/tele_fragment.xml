<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black"
    android:padding="15dp"
    android:paddingRight="400dp">

    <TextView
        android:id="@+id/tv_grey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:text="@string/tote_level"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffffff"
        android:textSize="20sp" />

    <Button
        android:id="@+id/btn_add_stack"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#ffffffff"
        android:nestedScrollingEnabled="true"
        android:onClick="addStack"
        android:text="@string/add_stack"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/can_level6"
        android:layout_marginBottom="118dp" />

    <CheckBox
        android:id="@+id/tote_level1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_grey"
        android:layout_marginBottom="20dp"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/tote_level_1"
        android:textColor="#ffffffff"
        android:textSize="20sp" />

    <CheckBox
        android:id="@+id/tote_level2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tote_level1"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="0dp"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/tote_level_2"
        android:textColor="#ffffffff"
        android:textSize="20sp" />

    <CheckBox
        android:id="@+id/tote_level3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tote_level2"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="0dp"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/tote_level_3"
        android:textColor="#ffffffff"
        android:textSize="20sp" />

    <CheckBox
        android:id="@+id/tote_level4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tote_level3"
        android:layout_marginBottom="20dp"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/tote_level_4"
        android:textColor="#ffffffff"
        android:textSize="20sp" />

    <CheckBox
        android:id="@+id/tote_level5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tote_level4"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="0dp"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/tote_level_5"
        android:textColor="#ffffffff"
        android:textSize="20sp" />

    <CheckBox
        android:id="@+id/tote_level6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tote_level5"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="0dp"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/tote_level_6"
        android:textColor="#ffffffff"
        android:textSize="20sp" />

    <CheckBox
        android:id="@+id/can_level2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/cans_level_2"
        android:textColor="#ffffffff"
        android:textSize="20sp"
        android:layout_below="@+id/tote_level1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:id="@+id/can_level3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/cans_level_3"
        android:textColor="#ffffffff"
        android:textSize="20sp"
        android:layout_below="@+id/tote_level2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:id="@+id/can_level4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/cans_level_4"
        android:textColor="#ffffffff"
        android:textSize="20sp"
        android:layout_below="@+id/tote_level3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:id="@+id/can_level5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/cans_level_5"
        android:textColor="#ffffffff"
        android:textSize="20sp"
        android:layout_below="@+id/tote_level4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:id="@+id/can_level6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/cans_level_6"
        android:textColor="#ffffffff"
        android:textSize="20sp"
        android:layout_below="@+id/tote_level5"
        android:layout_alignLeft="@+id/can_level5"
        android:layout_alignStart="@+id/can_level5" />

    <CheckBox
        android:id="@+id/can_level1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@drawable/box"
        android:paddingLeft="20dp"
        android:text="@string/cans_level_1"
        android:textColor="#ffffffff"
        android:textSize="20sp"
        android:layout_alignTop="@+id/tote_level1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/noodle"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffffff"
        android:textSize="20sp"
        android:layout_below="@+id/tote_level6"
        android:layout_centerHorizontal="true" />

</RelativeLayout>