<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.stormroboticsnj.frc_scouting_2015_user">
    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@drawable/logo" android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MatchActivity"
            android:label="@string/title_activity_match"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".qr"
            android:label="@string/title_activity_qr"></activity>
        <android:uses-permission
            android:name="android.permission.WRITE_EXTERNAL_STORAGE"
            android:maxSdkVersion="18" />
    </application>

</manifest>
